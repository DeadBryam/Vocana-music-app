{
  "@quasar/dotenv": {
    "env_development": ".env",
    "env_production": ".env",
    "common_root_object": "none",
    "create_env_files": true,
    "add_env_to_gitignore": true
  },
  "@quasar/icon-genie": {
    "minify_dev": "pngquant",
    "minify_build": "optipng",
    "cordova": {
      "background_color": "#FFFFFF",
      "splashscreen_type": "generate"
    },
    "build_always": false,
    "__internal": {
      "dev": {
        "spa": {
          "iconHash": "7c258b6fc5c75d73e89b6e0835972cb7"
        }
      },
      "build": {
        "cordova": {
          "iconHash": "7c258b6fc5c75d73e89b6e0835972cb7",
          "splashscreenHash": false,
          "splashscreenType": "generate",
          "backgroundColor": "#FFFFFF"
        }
      }
    }
  }
}